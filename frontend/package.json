{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:fast": "playwright test --config=playwright.config.fast.ts",
    "test:e2e:dev": "playwright test --config=playwright.config.dev.ts",
    "test:e2e:app": "playwright test tests/app.spec.ts",
    "test:e2e:app-fast": "playwright test tests/app-fast.spec.ts --config=playwright.config.dev.ts",
    "test:e2e:advanced": "playwright test tests/advanced.spec.ts",
    "test:e2e:advanced-fast": "playwright test tests/advanced-fast.spec.ts --config=playwright.config.dev.ts",
    "test:e2e:comprehensive": "playwright test --config=playwright.config.comprehensive.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:ai": "node scripts/test-ai-services.js",
    "test:ai:headed": "node scripts/test-ai-services.js --headed",
    "test:ai:debug": "node scripts/test-ai-services.js --debug",
    "test:ai:verbose": "node scripts/test-ai-services.js --verbose",
    "test:ai:frontend": "playwright test tests/ai-services.spec.ts --config=playwright.config.ai-services.ts",
    "test:ai:backend": "playwright test tests/ai-backend-api.spec.ts --config=playwright.config.ai-services.ts",
    "test:ai:integration": "playwright test tests/ai-integration.spec.ts --config=playwright.config.ai-services.ts",
    "test:ai:mobile": "playwright test tests/ai-services.spec.ts --config=playwright.config.ai-services.ts --project=ai-services-mobile",
    "test:phase1-week2": "playwright test --config=playwright.config.phase1-week2.ts",
    "test:phase1-week2:fast": "node scripts/run-phase1-week2-tests.js",
    "test:phase1-week2:headed": "playwright test --config=playwright.config.phase1-week2.ts --headed",
    "test:phase1-week2:debug": "playwright test --config=playwright.config.phase1-week2.ts --debug"
  },
